<Window x:Class="FlowEditor.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlowEditor"
        mc:Ignorable="d"
        Title="InputWindow" Height="Auto" Width="Auto">
    <StackPanel>
        <Label x:Name="LabelOutput">Lorem Ipsum</Label>
        <TextBox x:Name="TextBoxInput" KeyDown="TextBoxInput_KeyDown"></TextBox>
        <Button x:Name="ButtonConfirm" Click="ButtonConfirm_Click">OK</Button>
    </StackPanel>
</Window>
